{
  "manifest_version": 2,

  "background": {
    "persistent": false,
    "scripts": [ "scroll_percentage.js" ]
  },

  "name": "ScrollPercentage",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [ "scroll_percentage_analyze.js" ]
}
